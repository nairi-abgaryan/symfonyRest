{% extends 'front/layout/app.html.twig' %}

{% block content %}

<div class="container">
	<div class="row">
		<div class="col-md-4">
			<table class="table table-striped">
				{% for number in numbers %}
					<tr>
						<td>{{number.home|e}}</td>
						<td>{{number.mobile|e}}</td>
						<td>{{number.office|e}}</td>
						<td><a href="/edit/number/{{number.user_id}}" class="btn btn-xs btn-primary">edit</a></td>
						<td><a href="/delete/number/{{number.user_id}}" class="btn btn-xs btn-danger">delete</a></td>
					</tr>
				{% endfor %}
			</table>
		</div>
	</div>
</div>

{% endblock %}